#!/usr/bin/python
# -*- encoding: utf-8; py-indent-offset: 4 -*-

def check_wimax_ss_provis_kpi_arguments(params):
    description,settings = params
    args =''
    if "host" in settings:
        args += '%s ' % quote_shell_string(settings['host'])
    if "site" in settings:
        args += '%s ' % quote_shell_string(settings['site'])

    return args

active_check_info['wimax_ss_provis_kpi'] = {
    "command_line"        : '$USER1$/check_wimax_ss_provis_kpi $ARG1$',
    "argument_function"   : check_wimax_ss_provis_kpi_arguments,
    "service_description" : lambda params: "%s" % params[0],
    "has_perfdata"        :True,
}

